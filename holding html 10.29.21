<!DOCTYPE html>
<html lang="en">

<head>
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Cami's Coding Quiz</title>
    <link rel="stylesheet" type="text/css" href="./02-Homework/style.css" />
</head>

<body>
    <section id=introductionpage>
        <h1> Coding Quiz</h1>
        <h5>This is a quiz description and set of instructions</h5>
        <button id="startbtn">Start Quiz</button>
    </section>
    <!-- quiz-container -->
    <section id="quizoptions">
        <div id="quizcontainer">
            <!-- question container -->
            <h3 id="timerName" class="timekeeping"> Timer: <span></span> </h3>
            <h3 id="timer" class="timekeeping"> <span></span> </h3>
            <div class="buttons" id="introductionPage">
                <h2 id="insertquestions">""</h2>
                <div id="optionscontainer" class="options">
                    <h2 id="optionsLos">Question here</h2>
                    <ol type="A">
                        <button>
                            <li id="option0"></li>
                        </button>
                        <button>
                            <li id="option1"></li>
                        </button>
                        <button>
                            <li id="option2"></li>
                        </button>
                        <button>
                            <li id="option3"></li>
                        </button>

                    </ol>
    <section id="scoresection" class="keepingscore"></section>
    <h2 id="score-title">High Scores</h2>
    </div>
    </div>
    </div>


    </div>

    </section> 
</section>
    <!-- scoreboard section -->
   
</body>
<script src="https://code.jquery.com/jquery-3.5.1.min.js"></script>
<script type="text/javascript" src="./script.js"></script>

</html>
